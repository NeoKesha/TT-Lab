<base:BaseEditor x:Class="TT_Lab.Editors.Graphics.ModelEditor"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:TT_Lab.Editors.Graphics"
             xmlns:base="clr-namespace:TT_Lab.Editors"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="419*"/>
            <RowDefinition Height="31*"/>
        </Grid.RowDefinitions>
        <base:SceneEditor x:Name="SceneRenderer" Header="Model viewer"/>
        <Button x:Name="ReplaceButton" Content="Replace" Margin="0" Grid.Row="1"/>
    </Grid>
</base:BaseEditor>
