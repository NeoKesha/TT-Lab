<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net7.0-windows</TargetFramework>
    <UseWpf>true</UseWpf>
    <UseWindowsForms>True</UseWindowsForms>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <ApplicationIcon>Images\tt_lab_logo_v6_2k.ico</ApplicationIcon>
    <Nullable>enable</Nullable>
    <AnalysisLevel>latest</AnalysisLevel>
    <PlatformTarget>x64</PlatformTarget>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DebugType>none</DebugType>
    <DebugSymbols>false</DebugSymbols>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <NoWarn>1701;1702;CA1416</NoWarn>
    <TreatWarningsAsErrors>False</TreatWarningsAsErrors>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <NoWarn>1701;1702;CA1416</NoWarn>
    <TreatWarningsAsErrors>False</TreatWarningsAsErrors>
  </PropertyGroup>

  <PropertyGroup>
    <BuildDependsOn>
      $(BuildDependsOn);CopyOgreLibs
    </BuildDependsOn>
  </PropertyGroup>

  <Target Name="CopyOgreLibs" AfterTargets="AfterBuild">
    <ItemGroup>
      <OgreLibraries Include="$(MSBuildProjectDirectory)\Libraries\Native\Windows\*.dll"/>
    </ItemGroup>
    
    <Message Text="Copying Ogre libraries from $(MSBuildProjectDirectory)\Libraries\Native\Windows into resulting executable..." Importance="high" />
    <Copy SourceFiles="@(OgreLibraries)" DestinationFolder="$(MSBuildProjectDirectory)\$(OutputPath)" />
  </Target>

