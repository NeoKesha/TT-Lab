<UserControl x:Class="TT_Lab.Views.Editors.Instance.Cameras.BaseCameraView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:controls="clr-namespace:TT_Lab.Controls"
             xmlns:local="clr-namespace:TT_Lab.Views.Editors.Instance.Cameras"
             xmlns:viewmodels="clr-namespace:TT_Lab.ViewModels.Editors.Instance.Cameras"
             d:DataContext="{Binding Source={d:DesignInstance viewmodels:BaseCameraViewModel}}"
             mc:Ignorable="d"
             d:DesignHeight="300" d:DesignWidth="300">
    <GroupBox Header="Base Parameters">
        <StackPanel Orientation="Vertical">
            <controls:LabeledTextBox HorizontalAlignment="Stretch" TextBoxName="Unknown Integer" Text="{Binding UnkInt}"/>
            <controls:LabeledTextBox HorizontalAlignment="Stretch" TextBoxName="Unknown Float 1" Text="{Binding UnkFloat1}"/>
            <controls:LabeledTextBox HorizontalAlignment="Stretch" TextBoxName="Unknown Float 2" Text="{Binding UnkFloat2}"/>
        </StackPanel>
    </GroupBox>
</UserControl>
